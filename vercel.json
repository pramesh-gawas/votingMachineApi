// {

//   "rewrites":[
//     {"source":"*/(.*)","destination":"/"}
//   ]
// }

{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods":["GET","OPTIONS"],
      "headers":{"Access-Control-Allow-Origin":"*"}
    }
  ]
}